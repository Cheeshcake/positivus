---
import MainHead from "./main-head.astro";
import Nav from "../components/nav.astro";
import Footer from "../components/footer.astro"
interface Props {
  title?: string;
  description?: string;
}
const { title = "default", description = "default" } = Astro.props as Props;
---

<html lang="en">
  <MainHead title={title} description={description} />
  <Nav/>
  <body>
    <slot />
  </body>
  <Footer/>
</html>
